{
  "name": "Vipsy",
  "version": "2.5.0",
  "slug": "vipsy",
  "description": "Secure remote access to Home Assistant â€” TLS gateway, Cloudflare Tunnel, and WireGuard VPN for LAN access.",
  "url": "https://github.com/nky001/vipsy",
  "arch": ["armv7", "armhf", "aarch64", "amd64", "i386"],
  "init": false,
  "startup": "services",
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "privileged": ["NET_ADMIN", "SYS_ADMIN"],
  "full_access": true,
  "host_network": true,
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:shield-lock",
  "ports": {
    "443/tcp": 443,
    "3478/udp": 3478,
    "51820/udp": 51820
  },
  "ports_description": {
    "443/tcp": "HTTPS reverse proxy (Caddy)",
    "3478/udp": "TURN/STUN relay (coturn)",
    "51820/udp": "WireGuard VPN"
  },
  "devices": ["/dev/net/tun"],
  "map": ["ssl:ro", "config:rw"],
  "options": {
    "domain": "",
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "enable_turn": true,
    "tunnel_enabled": false,
    "vpn_subnet": "10.8.0.0/24",
    "vpn_port": 51820
  },
  "schema": {
    "domain": "str",
    "certfile": "str",
    "keyfile": "str",
    "enable_turn": "bool",
    "tunnel_enabled": "bool",
    "vpn_subnet": "str",
    "vpn_port": "int"
  }
}
